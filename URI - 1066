#include <stdio.h>
 
int main() {
    int a;
    int i;
    int pares = 0, impares = 0, positivos = 0, negativos = 0;
    for (i = 0; i < 5; i++){
        scanf("%d", &a);
        if (a > 0){
            positivos++;
        }
        if (a < 0){
            negativos++;
        }
        if (a % 2 == 0){
            pares++;
        }
        if (a % 2 != 0){
            impares++;
        }
    }
    printf("%d valor(es) par(es)\n", pares);
    printf("%d valor(es) impar(es)\n", impares);
    printf("%d valor(es) positivo(s)\n", positivos);
    printf("%d valor(es) negativo(s)\n", negativos);
    return 0;
}
