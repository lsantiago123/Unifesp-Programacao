#include <stdio.h>
int main() {
    int funcionario;
    int horastrab;
    double ganhoporhora;
    double salario;
    scanf("%d",&funcionario);
    scanf("%d",&horastrab);
    scanf("%lf",&ganhoporhora);
    printf("NUMBER = %d\n",funcionario);
    salario=horastrab*ganhoporhora;
    printf("SALARY = U$ %.2lf\n",salario);
    return 0;
}
