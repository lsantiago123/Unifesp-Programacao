#include <stdio.h>
#include <string.h>
int main(){
    int m, n, soma, i;
    char x[20];
    while(1){
        scanf ("%d %d", &m, &n);
        if (m == 0 && n == 0){
           break;
        }
        soma = m + n;
        sprintf (x, "%d", soma);
        for (i = 0; i < strlen(x); i++){
            if (x[i] != '0'){
                printf ("%c", x[i]);
            }
        }
        printf("\n");
    }
    return 0;
}
