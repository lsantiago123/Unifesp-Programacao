#include <stdio.h>
 
int main() {
    int horai, horaf, resultado;
    scanf ("%d %d", &horai, &horaf);
    if (horai < horaf){
        resultado = horaf - horai;
        printf ("O JOGO DUROU %d HORA(S)\n", resultado);
    }
    else{
        if (horai == horaf){
            printf ("O JOGO DUROU 24 HORA(S)\n");
        }
        else{
            resultado = (24 - horai + horaf);
            printf ("O JOGO DUROU %d HORA(S)\n", resultado);
        }
    }
    return 0;
}
