#include <stdio.h>
 
int main() {
    char subfilo[30], classe[30], alimentar[30];
    scanf("%s %s %s", &subfilo, &classe, &alimentar);
    if (subfilo[0] == 'v'){
        if (classe[0] == 'a'){
            if (alimentar[0] == 'c'){
                printf ("aguia\n");
            }
            else{
                printf ("pomba\n");
            }
        }
        else{
            if (alimentar[0] == 'o'){
                printf ("homem\n");
            }
            else{
                printf ("vaca\n");
            }
        }
    }
    else{
        if (classe[0] == 'i'){
            if (alimentar[2] == 'm'){
                printf ("pulga\n");
            }
            else{
                printf ("lagarta\n");
            }
        }
        else{
            if (alimentar[0] == 'h'){
                printf ("sanguessuga\n");
            }
            else{
                printf ("minhoca\n");
            }
        }
    }
    return 0;
}
