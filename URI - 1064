#include <stdio.h>
 
int main() {
    double a;
    int i;
    double media, soma = 0;
    int positivo = 0;
    for (i = 0; i < 6; i++){
        scanf("%lf", &a);
        if (a > 0){
            positivo++;
            soma += a;
        }
    }
    media = soma/positivo;
    printf("%d valores positivos\n", positivo);
    printf ("%.1lf\n", media);
    return 0;
}
